void setup() {
  Serial.begin(9600);
  while (!Serial) {
    ; // wait for serial port to connect. Needed for native USB port only
  }

}

void loop() {
  int max_step = 10000;
  for(int x=1;x<max_step;x++){
    Serial.print(x*int(sqrt(x)));
    Serial.print(", ");
    Serial.println(max_step/x);
    delay(1000);
  }
  
}